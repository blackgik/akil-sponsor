    "@babel/preset-env": "7.24.4",
    "@babel/preset-typescript": "7.24.1",
    "@babel/runtime": "7.24.4",
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.7",
    "@types/core-js": "2.5.8",
    "@types/cors": "2.8.17",
