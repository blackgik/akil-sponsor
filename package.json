    "core-js": "3.36.1",
    "cors": "2.8.5",
    "dotenv": "^16.4.5",
    "express": "4.19.2",
    "helmet": "7.1.0",
    "husky": "9.0.11",
    "ioredis": "^5.3.2",
    "joi": "^17.12.2",
